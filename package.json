{
  "name": "ob.js",
  "version": "0.1.4",
  "description": "ob.js comes from vue.js and is used to observe/watch Object/Array/Class",
  "main": "dist/ob.js",
  "scripts": {
    "test": "mocha",
    "lint": "eslint src/**",
    "build": "rm -rf dist && npm run rollup && MIN=min npm run rollup",
    "rollup": "rollup --config --environment NODE_ENV:production --sourcemap",
    "dev": "webpack-dev-server --config dev/webpack.config.js --content-base dev/ --inline --hot --port 3031",
    "demo": "cp dist/ob.min.js demo && webpack-dev-server --config demo/webpack.config.js --content-base demo/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lon3/ob.js.git"
  },
  "keywords": [
    "ob.js",
    "ob",
    "observe",
    "observer",
    "watch",
    "watcher",
    "computed",
    "vue",
    "vue.js"
  ],
  "author": "longhaohe@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "chai": "^3.5.0",
    "eslint": "^2.10.2",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.0",
    "eslint-plugin-standard": "^1.3.2",
    "mocha": "^2.5.3",
    "rollup": "^0.26.3",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-uglify": "^0.3.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "bugs": {
    "url": "https://github.com/lon3/ob.js/issues"
  },
  "homepage": "https://github.com/lon3/ob.js#readme"
}
